{
  "expo": {
    "name": "EatLater Vendors",
    "description": "Order food from your favorite vendors and restaurants",
    "slug": "vendor",
    "owner": "EatLater",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-config/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload photos of your listings"
        }
      ]
    ],
    "splash": {
      "image": "./assets/app-config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/app-config/favicon.png"
    }
  }
}
